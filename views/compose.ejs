<%- include("partials/header"); -%>


<h1>Compose</h1>

<form class="item" action="/compose" method="post">

  <div class="form-group">
    <div class="mb-3">
      <label for="newTitle" class="form-label">Title</label>
      <input type="text" class=form-control name="newTitle" autocomplete="off">
    </div>
  </div>
  <div class="form-group">
    <div class="mb-3">
      <label for="newPost" class="form-label">Post</label>
      <textarea class="form-control" name="newPost" rows="5"></textarea>
    </div>
  </div>
  <button class="btn btn-primary btn-sm" type="sumbit" name="button">Publish</button>


</form>


<%- include("partials/footer"); -%>
